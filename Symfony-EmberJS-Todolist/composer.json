{
    "name": "dustinwhittle/symfony-emberjs-sunshinephp-2013",
    "description": "Symfony EmberJS at SunshinePHP 2013",
    "name": "dustinwhittle/webapp",
    "description": "dustinwhittle.com",
    "keywords": ["dustinwhittle"],
    "homepage": "https://dustinwhittle.com/",
    "type": "app",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Whittle",
            "email": "dustin.whittle@gmail.com",
            "homepage": "https://dustinwhittle.com/"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/symfony",
        "issues": "https://github.com/dustinwhittle/Symfony-EmberJS-SunshinePHP-2013/issues"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "minimum-stability": "dev",
    "require": {

        "php": ">=5.4.5",

        "ext-apc": ">=3.1.12",
        "ext-bz2": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-intl": ">=1.1.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-Phar": ">=2.0.1",
        "ext-posix": "*",
        "ext-session": "*",
        "ext-simplexml": "*",
        "ext-spl": "*",
        "ext-xml": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "ext-zlib": "*",

        "monolog/monolog": "1.3.1",

        "kriswallsmith/assetic": "dev-master",
        "kriswallsmith/buzz": "dev-master",

        "swiftmailer/swiftmailer": "dev-master",

        "twig/twig": "dev-master",
        "twig/extensions": "dev-master",

        "symfony/symfony": "dev-master",
        "symfony/swiftmailer-bundle": "dev-master",
        "symfony/monolog-bundle": "dev-master",
        "symfony/assetic-bundle": "dev-master",
        "symfony/twig-bundle": "dev-master",

        "doctrine/common": "2.3.x-dev",
        "doctrine/dbal": "2.3.x-dev",
        "doctrine/orm": "2.3.x-dev",
        "doctrine/data-fixtures": "dev-master",
        "doctrine/migrations": "dev-master",
